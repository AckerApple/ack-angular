{
  "name": "ack-angular",
  "version": "0.6.9",
  "description": "Extra special directives, components, providers and pipes to aide in tackling everyday interface development needs in Angular2",
  "main": "ack-angular/index.js",
  "typings": "ack-angular/index.d.ts",
  "scripts": {
    "build:index": "pug example/src/index.pug --out example/www/",
    "build:css": "npm run build:css:ack-css-boot && npm run build:css:csshake && npm run build:css:dist && npm run build:css:example",
    "build:css:ack-css-boot": "ack-sass node_modules/ack-css-boot/scss/ack-css-boot.scss ack-angular/ack-css-boot.css --production",
    "build:css:csshake": "ack-sass node_modules/csshake/scss/csshake.scss ack-angular/csshake.css --production",
    "build:css:dist": "ack-sass scss/ack-angular.scss ack-angular/ack-angular.css --production",
    "build:css:example": "ack-sass scss/ack-angular.scss example/www/styles.css --production",
    "install:example": "npm install --prefix example",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:dist:tsc": "tsc --declaration --project src",
    "build:dist": "ngc --declaration --project src",
    "watch:dist": "watch \"npm run build:dist && npm run compile:aot\" src/ --ignoreDirectoryPattern=/pugs/",
    "ack-webpack": "ack-webpack",
    "build:assets": "npm run build:assets:src && npm run build:assets:example",
    "build:assets:src": "ack-pug-bundler src/pugs/ src/templates/ --oneToOne --outType ts",
    "build:assets:example": "ack-pug-bundler example/src/pugs/ example/src/templates/ --oneToOne --outType ts",
    "watch:assets": "npm run build:assets:src -- --watch & npm run build:assets:example -- --watch",
    "watch": "npm run watch:dist & npm run watch:assets & npm run watch:js",
    "watch:js": "ack-webpack example/index.ts example/www/index.js --project example/tsconfig.json --modules . --watch --browser",
    "compile:prefx": "npm run compile:prefx:src && npm run compile:prefx:example",
    "compile:prefx:src": "ack-angular-fx --select 200,childStag --effects fade --out src/prefx.ts",
    "compile:prefx:example": "ack-angular-fx --select absoluteSwap500,200,500,childStag --effects slide,fade,zoom --out example/src/prefx.ts",
    "compile:aot": "npm run compile:aot:copy && ngc -p example/tsconfig.aot.json",
    "compile:aot:copy": "ncp src example/src/ack-angular && ncp package.json example/src/ack-angular/package.json",
    "build:js": "ack-webpack example/index.aot.ts example/www/index.js --production --project tsconfig.json --modules ../",
    "build": "npm run build:index && npm run build:css && npm run build:assets && npm run compile:dist:package && npm run compile:prefx && npm run build:dist && npm run compile:aot && npm run build:js",
    "compile:dist:package": "node scripts/update-dist-package.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AckerApple/ack-angular.git"
  },
  "keywords": [
    "ng2",
    "angular2",
    "angular",
    "directives",
    "components",
    "pipes",
    "providers"
  ],
  "author": "Acker Dawn Apple",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AckerApple/ack-angular/issues"
  },
  "homepage": "https://github.com/AckerApple/ack-angular#readme",
  "devDependencies": {
    "@angular/animations": "^4.3.0",
    "@angular/common": "^4.3.0",
    "@angular/compiler": "^4.3.0",
    "@angular/compiler-cli": "^4.3.0",
    "@angular/core": "^4.3.0",
    "@angular/forms": "^4.3.0",
    "@angular/http": "^4.3.0",
    "@angular/platform-browser": "^4.3.0",
    "@angular/platform-browser-dynamic": "^4.3.0",
    "@angular/router": "^4.3.0",
    "ack-angular-fx": "^1.2.3",
    "ack-css-boot": "^1.2.42",
    "ack-pug-bundler": "^1.3.8",
    "ack-sass": "^1.1.0",
    "ack-webpack": "^1.1.36",
    "ack-x": "^1.2.22",
    "csshake": "^1.5.1",
    "localforage": "^1.5.0",
    "ng2-page-scroll": "^4.0.0-beta.9",
    "pug": "^2.0.0-rc.2",
    "pug-cli": "^1.0.0-alpha6",
    "pug-loader": "^2.3.0",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.4.1",
    "ts-loader": "^2.2.0",
    "typescript": "^2.3.4",
    "webpack": "^2.3.2",
    "zone.js": "^0.8.12"
  },
  "jsDependencies": {
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.4.0",
    "zone.js": "^0.8.11",
    "ack-angular-fx": "^1.1.2",
    "@angular/common": "^4.3.0",
    "@angular/core": "^4.3.0",
    "@angular/platform-browser": "^4.3.0",
    "@angular/platform-browser-dynamic": "^4.3.0",
    "localforage": "^1.5.0",
    "ack-x": "^1.2.22",
    "@angular/http": "^4.3.0",
    "@angular/forms": "^4.3.0"
  },
  "private": true,
  "dependencies": {
    "node-dir": "^0.1.17",
    "ts-node": "^3.1.0"
  }
}
