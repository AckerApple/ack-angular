{
  "name": "example",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "ack-webpack": "ack-webpack",
    "install:example:js": "ack-webpack install github:ackerapple/ack-angular --out src/js_modules --no-save",
    "compile:example:prefx": "ack-angular-fx --select absoluteSwap500,200,500 --effects slide,fade --out src/prefx.ts",
    "compile:aot": "ngc -p tsconfig.aot.json",
    "build:example:js": "ack-webpack index.aot.ts www/index.js --production --project tsconfig.json --modules .",
    "build:example": "npm run build:example:js && npm run build:example:index && npm run build:example:css",
    "build:example:dist": "ncp . src/ack-angular/ --stopOnErr",
    "watch:example:js": "ack-webpack index.aot.ts www/index.js --watch --browser --project tsconfig.json --modules ../",
    "watch:example:js:jit": "ack-webpack index.ts www/index.js --watch --browser --project tsconfig.json --modules ../",
    "build:example:index": "pug src/index.pug --out www/",
    "build:example:css": "ack-sass src/styles.scss www/styles.css --production",
    "watch:example:css": "ack-sass src/styles.scss www/styles.css --watch",
    "watch": "npm run watch:assets & npm run watch:example:js:jit & npm run watch:example:css",
    "build": "npm run compile:example:prefx && npm run compile:example:aot && npm run build:dist && npm run build:example",
    "build:example:assets": "ack-pug-bundler src/pugs/ src/templates/ --oneToOne --outType ts",
    "build:assets": "npm run build:src:assets && npm run build:example:assets"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@angular/animations": "^4.0.0",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "@types/node": "^7.0.11",
    "ack-angular": "github:ackerapple/ack-angular",
    "ack-angular-fx": "^1.1.0",
    "ack-css-boot": "^1.2.27",
    "ack-pug-bundler": "^1.3.0",
    "ack-sass": "^1.0.13",
    "ack-webpack": "^1.1.16",
    "ng2-page-scroll": "^4.0.0-beta.6",
    "pug": "^2.0.0-beta11",
    "pug-cli": "^1.0.0-alpha6",
    "pug-loader": "^2.3.0",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.2.0",
    "ts-loader": "^2.0.3",
    "typescript": "^2.2.1",
    "ui-router-ng2": "^1.0.0-beta.4",
    "webpack": "^2.3.2",
    "zone.js": "^0.8.5"
  }
}
